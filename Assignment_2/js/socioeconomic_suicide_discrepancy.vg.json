{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "description": "Age-standardized suicide rate by socioeconomic type for 2010 and 2023",
  "height": 400,
  "width": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/DustinKeene/FIT3179/refs/heads/main/Assignment_2/data/socioeconomic_standardised_suicide_rate.csv"
  },
  "transform": [
    {
      "filter": {
        "field": "year",
        "oneOf": [2010, 2023]
      }
    }
  ],
  "encoding": {
    "x": {
      "field": "age_standardised_suicide_rate",
      "type": "quantitative",
      "title": "Age-Standardised Suicide Rate"
    },
    "y": {
      "field": "socioeconomic_type",
      "type": "nominal",
      "title": "Socioeconomic Type",
      "axis": {
        "offset": 30,
        "ticks": false,
        "minExtent": 50, 
        "domain": false
      }
    }
  },
  "layer": [
    {
      "mark": "line",
      "encoding": {
        "detail": {
          "field": "socioeconomic_type",
          "type": "nominal"
        },
        "color": {"value": "#db646f"}
      }
    },
    {
      "mark": {
        "type": "point",
        "filled": true
      },
      "encoding": {
        "color": {
          "field": "year",
          "type": "ordinal",
          "scale": {
            "domain": [2010, 2023],
            "range": ["#e6959c", "#911a24"]
          },
          "title": "Year  "
        },
        "size": {"value": 200},
        "opacity": {"value": 1},
        "tooltip": [
          {"field": "socioeconomic_type", "type": "nominal", "title": "Socioeconomic Type"},
          {"field": "age_standardised_suicide_rate", "type": "quantitative", "title": "Suicide Rate"},
          {"field": "year", "type": "ordinal", "title": "Year"}
        ]
      }
    }
  ],
  "config": {
    "background": "transparent",
    "view": {"stroke": null},
    "axis": {"grid": false}
  }
}